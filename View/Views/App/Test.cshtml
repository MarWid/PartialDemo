@model View.Models.Person

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Test</title>
</head>
<body>
    <div>Navn @Model.Navn</div> @*Behøver ikke bruges længere*@
    <input type="text" value="@Model.Navn" />@*Behøver ikke bruges længere skal holde styr på alt muligt*@
    <hr />

    @Html.DisplayNameFor(m => m.Navn) @*Viser navnet på egenskaben*@

